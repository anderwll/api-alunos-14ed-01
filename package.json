{
  "name": "api-alunos-prisma",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./src",
    "build": "npx tsc",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "test": "jest --silent --noStackTrace --runInBand --verbose",
    "test:verbose": "jest --verbose --runInBand --detectOpenHandles",
    "test:coverage": "jest --runInBand --coverage",
    "test:watch": "jest --silent --noStackTrace --runInBand --watchAll",
    "test:unit": "jest -c jest.unit.config.ts --silent --noStackTrace --verbose"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.7.1",
    "@types/uuid": "^9.0.4",
    "prisma": "^5.3.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
